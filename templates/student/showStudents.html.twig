<table>
    <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>
    {% for student in students %}
        <tr>
            <td>
                {{ student.id }}
            </td>
            <td>
                {{ student.firstName }}
            </td>
            <td>
                {{ student.lastName }}
            </td>
        </tr>
    {% endfor %}
</table>